{
  "name": "rz-methods",
  "version": "3.0.0",
  "description": "A utility library with tree-shakeable methods for both Node.js and browser environments",
  "main": "dist/cjs/index.min.js",
  "module": "dist/esm/index.min.js",
  "umd:main": "dist/umd/index.min.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:bundle && npm run build:types",
    "build:clean": "rimraf dist",
    "build:bundle": "rollup -c",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/types",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src test --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major"
  },
  "keywords": [
    "utility",
    "methods",
    "typescript",
    "tree-shaking"
  ],
  "author": "Roc-zhou",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Roc-zhou/rz-methods.git"
  },
  "bugs": {
    "url": "https://github.com/Roc-zhou/rz-methods/issuess"
  },
  "homepage": "https://github.com/Roc-zhou/rz-methods/blob/main/README.md",
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^28.0.8",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "rollup": "^3.20.2",
    "rollup-plugin-eslint": "^7.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-jest": "^29.1.0",
    "typescript": "~5.1.6"
  },
  "sideEffects": false
}
